<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Servlets.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstudianteCRUD</servlet-name>
        <servlet-class>Servlets.EstudianteCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TutorCRUD</servlet-name>
        <servlet-class>Servlets.TutorCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarreraCRUD</servlet-name>
        <servlet-class>Servlets.CarreraCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MateriaCRUD</servlet-name>
        <servlet-class>Servlets.MateriaCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DimensionesCRUD</servlet-name>
        <servlet-class>Servlets.DimensionesCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CriteriosCRUD</servlet-name>
        <servlet-class>Servlets.CriteriosCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmpresaCRUD</servlet-name>
        <servlet-class>Servlets.EmpresaCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PracticasCRUD</servlet-name>
        <servlet-class>Servlets.PracticasCRUD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/inicio.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstudianteCRUD</servlet-name>
        <url-pattern>/estudiante.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TutorCRUD</servlet-name>
        <url-pattern>/tutor.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarreraCRUD</servlet-name>
        <url-pattern>/carrera.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MateriaCRUD</servlet-name>
        <url-pattern>/materia.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DimensionesCRUD</servlet-name>
        <url-pattern>/dimensiones.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CriteriosCRUD</servlet-name>
        <url-pattern>/criterios.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmpresaCRUD</servlet-name>
        <url-pattern>/empresa.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PracticasCRUD</servlet-name>
        <url-pattern>/practicas.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
